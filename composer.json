{
    "name": "drystack/components",
    "authors": [
        {
            "name": "Lorenzo Cattaneo",
            "email": "info@lorenzocattaneo.com"
        }
    ],
    "license": "AGPL-3.0-only",
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0",
        "livewire/livewire": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Drystack\\Components\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Drystack\\Components\\ComponentsServiceProvider"
            ]
        }
    }
}
